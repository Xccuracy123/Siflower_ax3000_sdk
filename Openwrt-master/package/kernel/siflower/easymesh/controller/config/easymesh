config easymesh 'config'
        option device_name 'my_device'
        option topo_request_interval '30'
        option find_controller_timeout '2'
        option lag_marginal_threshold '3'
        option roaming_threshold '60'
        option log_level '1'

config easymesh 'status'
        option enable '0'
        option wan 'none'
        option management_mode 'controller'
        option backhaul_conn_type 'wired'
        option conn_status 'false'
        option is_first_load 'false'
        option is_set_mode 'false'
        option can_enable_wps 'false'
        option can_enable_conn 'false'
        option can_update_wifi_config 'false'
