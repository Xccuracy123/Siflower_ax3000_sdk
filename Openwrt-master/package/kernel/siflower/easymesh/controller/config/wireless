
config wifi-device 'radio0'
        option type 'mac80211'
        option country 'CN'
        option txpower_lvl '2'
        option txpower '25'
        option channel '149'
        option channels '36-165'
        option band '5G'
        option hwmode '11a'
        option noscan '1'
        option netisolate '0'
        option max_all_num_sta '64'
        option rd_disabled '1'
        option path 'soc/0.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'
        option htmode 'HE80'
        option disabled '0'

config wifi-iface 'default_radio0'
        option device 'radio0'
        option network 'lan'
        option mode 'ap'
        option ssid 'SiWiFi-test'
        option encryption 'sae-mixed'
        option key '12345678'
        option hidden '0'
        option ifname 'wlan0'
        option wpa_group_rekey '36000'
        option isolate '0'
        option group '1'
        option disable_input '0'
        option wps_pushbutton '1'
        option wps_label '0'
        option multi_ap '2'
        option multi_ap_backhaul_ssid 'SiWiFi-test_bh'
        option multi_ap_backhaul_key '12345678'
        option ieee80211k '1'
        option ieee80211v '1'
        option bss_transtion '1'
        option ieee80211r '1'
        option disabled '0'
        option max_inactivity '30'

config wifi-iface 'default_radio01'
        option device 'radio0'
        option network 'lan'
        option mode 'ap'
        option ssid 'SiWiFi-test_bh'
        option encryption 'ccmp+psk2'
        option key '12345678'
        option hidden '0'
        option ifname 'wlan0-1'
        option wpa_group_rekey '36000'
        option isolate '0'
        option group '1'
        option disable_input '0'
        option wps_pushbutton '0'
        option wps_label '0'
        option multi_ap '1'
        option ieee80211k '1'
        option ieee80211v '1'
        option bss_transtion '1'
        option ieee80211r '1'
        option disabled '0'
        option max_inactivity '30'

config wifi-iface 'default_radio02'
        option device 'radio0'
        option ifname 'wlan0-2'
        option mode 'sta'
        option network 'lan'
        option wds '1'
        option wpa_pushbutton '1'
        option wps_config 'push_button'
        option multi_ap '1'
        option ssid 'easymesh'
        option encryption 'psk2+ccmp'
        option key '12345678'
        option disabled '1'

config wifi-device 'radio1'
        option type 'mac80211'
        option country 'CN'
        option txpower_lvl '2'
        option txpower '20'
        option channel '1'
        option band '2.4G'
        option hwmode '11g'
        option noscan '1'
        option netisolate '0'
        option max_all_num_sta '64'
        option rd_disabled '0'
        option path 'soc/0.pcie/pci0000:00/0000:00:00.0/0000:01:00.1'
        option htmode 'VHT20'
        option disabled '0'

